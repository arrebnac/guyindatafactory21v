							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "xmao_incremental_sink_table",
						"type": "DatasetReference",
						"parameters": {
							"DBName": "Incremental_Demo",
							"SchemaName": "dbo",
							"TableName": {
								"value": "@pipeline().parameters.TableName",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "Stored procedure1",
				"type": "SqlServerStoredProcedure",
