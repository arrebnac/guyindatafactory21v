							},
							"type": "String"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "guyinsql",
					"type": "LinkedServiceReference",
					"parameters": {
						"DBName": "Incremental_Demo"
					}
				}
			}
		],
		"parameters": {
